<%include ../layout/header.ejs %>

<table border="1">
  <tr>
    <th>user ID</th>
    <th>user name</th>
  
    <th>Actions</th>
  </tr>

  <% users.forEach(function(user){ %>
  <tr>
    <td><%= user._id %></td>
    <td><%= user.name %></td>
    <td><img src='/images/<%= user.image %>'> </td>
    <td>
      <a href="/users/delete/<%= user._id %>">Delete</a>
      <a href="/users/edit/<%= user._id %>">Edit</a>
     </td>
  </tr>
  <%})%>
</table>
<%include ../layout/footer.ejs %>
